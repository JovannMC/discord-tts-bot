{
  "name": "discord-tts-bot",
  "version": "4.2.0",
  "description": "A simple Discord TTS Bot that uses Google Translate and other TTS APIs. Forked by JovannMC",
  "main": "./src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "debug": "node ./src/app.js --debug",
    "dev": "nodemon --exec npm start",
    "lint:errors-only": "npx eslint src --quiet",
    "lint": "npx eslint src",
    "deploy": "node ./src/command-deployer.js",
    "start-sharded": "node ./src/sharded-client.js"
  },
  "engines": {
    "node": ">=16.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JovannMC/discord-tts-bot.git"
  },
  "author": "moonstar-x",
  "contributors": [
    {
      "name": "JovannMC",
      "email": "jovannmc@femboyfurry.net"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JovannMC/discord-tts-bot/issues"
  },
  "homepage": "https://github.com/JovannMC/discord-tts-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@greencoast/discord.js-extended": "^2.2.3",
    "@greencoast/logger": "^1.1.1",
    "axios": "^1.7.5",
    "deepmerge": "^4.3.1",
    "discord.js": "^13.17.1",
    "google-tts-api": "^2.0.2",
    "level": "^7.0.1",
    "redis": "^4.7.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-greencoast": "^0.0.5",
    "nodemon": "^3.1.4"
  }
}
